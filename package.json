{
	"name": "react-design-editor",
	"version": "0.0.76",
	"description": "Design Editor Tools with React.js + ant.design + fabric.js",
	"main": "dist/react-design-editor.cjs.js",
	"module": "dist/react-design-editor.es.js",
	"unpkg": "dist/react-design-editor.umd.js",
	"typings": "dist/types/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest --watch",
		"test:integration": "vitest run src/__tests__/integration/",
		"test:e2e": "vitest run src/__tests__/e2e/",
		"build": "npm run clean:dist && webpack --node-env production --config webpack.prod.js && typedoc",
		"build:lib": "npm run clean:dist && vite build && tsc --project tsconfig.build.json",
		"build:types": "vite build && tsc --project tsconfig.build.json",
		"start": "npm install && npm run start:dev",
		"start:dev": "webpack serve --config webpack.dev.js",
		"serve": "http-server docs -p 4001",
		"ghpages": "npm run build && node scripts/ghpages",
		"deploy": "npm run build:lib && npm publish",
		"lint": "eslint src --ext .ts,.tsx --fix",
		"lint:all": "eslint --ext .ts,.tsx src/",
		"lint:check": "eslint --ext .ts,.tsx src/",
		"lint:fix": "eslint --ext .ts,.tsx src/ --fix",
		"format": "prettier --write \"src/**/*.{ts,tsx,css,less,json,md}\"",
		"format:all": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,css,less,json,md}\"",
		"format:fix": "prettier --write \"src/**/*.{ts,tsx,css,less,json,md}\"",
		"typecheck": "tsc --noEmit",
		"validate": "npm run typecheck && npm run lint:check && npm run test:coverage",
		"clean": "node scripts/cleanup.js",
		"clean:all": "node scripts/cleanup.js",
		"clean:dist": "node scripts/clean",
		"audit:deps": "npm audit && npm outdated",
		"audit:todos": "node scripts/scan-todos.js",
		"quality": "npm run typecheck && npm run lint:all && npm run test",
		"tsc": "tsc --project tsconfig.build.json",
		"typedoc": "typedoc",
		"prepare": "husky",
		"precommit": "lint-staged"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/salgum1114/react-design-editor.git"
	},
	"keywords": [
		"React.js",
